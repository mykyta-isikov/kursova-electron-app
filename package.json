{
  "name": "viyskkomat",
  "productName": "Viyskkomat",
  "version": "0.1.0",
  "description": "Node.js + MongoDB app",
  "main": "main.js",
  "scripts": {
    "test": "echo Howdy",
    "start": "electron-forge start",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . viyskkomat --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Viyskkomat\"",    
    "package-linux": "electron-packager . viyskkomat --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Mykyta Isikov",
  "license": "ISC",
  "keywords": [],
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.6",
    "electron-packager": "^15.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "kursova_electron_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
